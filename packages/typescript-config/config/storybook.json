{
  "compilerOptions": {
    "isolatedDeclarations": false,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "outDir": "${configDir}/dist/storybook",
    "tsBuildInfoFile": "${configDir}/dist/storybook.tsbuildinfo",
    "types": [ "node", "vitest/globals" ]
  },
  "extends": "./settings.json",
  "include": [
    "${configDir}/.storybook/**/*.ts",
    "${configDir}/src/**/*.stories.{ts,tsx,mts,mtsx}"
  ]
}
